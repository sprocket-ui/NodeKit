{
  "$schema": "https://turbo.build/schema.json",
  "daemon": false,
  "tasks": {
    "build": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "build:*": {
      "cache": false,
      "outputs": ["dist/**"],
      "dependsOn": ["^build:*"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:*": {
      "cache": false,
      "persistent": true
    }
  }
}
