    "build:react": "turbo run build --filter='@sprocketui-react/*' --no-cache --parallel"
  },
  "packageManager": "pnpm@9.15.3",
  "devDependencies": {
    "@changesets/cli": "^2.29.2",
    "turbo": "^2.5.0",
    "vitest": "^3.1.1"
  }
}
