{
  "name": "sprocketui",
  "private": true,
  "license": "MIT",
  "scripts": {
    "changeset": "changeset",
    "test": "vitest",
    "dev": "turbo run dev",
    "dev:react": "turbo watch dev --filter=@sprocketui-react/*",
    "build": "turbo run build",
